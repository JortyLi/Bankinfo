//allStr = PBL201786|PBL200711|PBL200715
function getUnFavourite(favStr,allStr){
	var index = 0;
	var tempArr = new Array();
	var favString = ',';
	for (j = 0; j<favStr.length; j++) {
		var t2 = favStr[j];
		favString = favString+t2 +',';
	} 
	for (i=0; i<allStr.length; i++) {
		var t1 = allStr[i];
 		if(favString.indexOf(t1)=='-1'){
	 		tempArr[index]=t1;
			index++;
 		}
	}
	return tempArr;
}

//allStr = PBL0200~PBL201786|PBL0200~PBL200711|PBL0200~PBL200715
function getUnFavourite_new(favStr,allStr){
	var index = 0;
	var tempArr = new Array();
	var favString = ',';
	for (j = 0; j<favStr.length; j++) {
		var t2 = favStr[j];
		favString = favString+t2 +',';
	} 
	for (i=0; i<allStr.length; i++) {
		var t1 = allStr[i];
		if(t1.indexOf("~") == "-1"){
			continue;
		}
		var arrTmp = t1.split("~")
 		if(favString.indexOf(arrTmp[1])=='-1'){
	 		tempArr[index]=t1;
			index++;
 		}
	}
	return tempArr;
}


function getUnFavourite_limit(favStr,allStr,num){
	var index = 0;
	var tempArr = new Array();
	var favString = ',';
	for (j = 0; j<favStr.length; j++) {
		var t2 = favStr[j];
		favString = favString+t2 +',';
	} 
	for (i=0; i<allStr.length; i++) {
		var t1 = allStr[i];
		if(t1.indexOf("~") == "-1"){
			continue;
		}
		var arrTmp = t1.split("~")
 		if(favString.indexOf(arrTmp[1])=='-1'){
	 		tempArr[index]=t1;
			index++;
 		}
 		if(index >= num){
 			break;
 		}
	}
	return tempArr;
}


