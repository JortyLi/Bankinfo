function getAtomService(){
	
	var hisAtomService = window.sessionStorage.getItem(dse_sessionId); 
	var id1;
	var id3;
	var name2;
	var name3;
	var menuData = new Array();
	var num = 0;
	var Sys={};
    var ua = window.navigator.userAgent.toLowerCase();
    var s;
    try{
    	(s = ua.match(/msie ([\d.]+)/))?Sys.ie = s[1]:(s = ua.match(/firefox\/([\d.]+)/))?Sys.firefox = s[1]:(s = ua.match(/chrome\/([\d.]+)/))?Sys.chrome = s[1]:(s = ua.match(/opera.([\d.]+)/))?Sys.opera = s[1]:(s = ua.match(/version\/([\d.]+).*safari/))?Sys.safari = s[1]:0;
	}catch(e){
		//无需处理
	}
	if(hisAtomService!=null&&hisAtomService!=""&&hisAtomService!="undefined"){
		try{
		
			menuData[0] = "";
		
			menuData[1] = "";
		
			menuData[2] = "";
		
			menuData[3] = "";
		
			menuData[4] = "";
		
			menuData[5] = "";
		
			menuData[6] = "";
		
			menuData[7] = "";
		
			menuData[8] = "";
		
			menuData[9] = "";
		
			menuData[10] = "";
		
			menuData[11] = "id=PBL203001name=nullurl=nullid=PBL20300101name=nullurl=nullid=PBL200780name=nullurl=nullid=PBL200781name=nullurl=nullid=PBL200782name=nullurl=null";
		
			menuData[12] = "id=PBL201700name=nullurl=nullid=PBL201786name=nullurl=null";
		 
		var mLength = menuData.length;
		var hisAtomServiceArray = hisAtomService.split("&");
		var size=hisAtomServiceArray.length;
		if(size > 0){	
			for(var m = 0; m < mLength; m ++){
				var hrefData = new Array();
				menuDataTmp = menuData[m];
				var count = 0;
				$("#div_"+menuDataTmp.substring(menuDataTmp.indexOf('P'),menuDataTmp.indexOf('n'))+"_right").html("");
				for(var index=0;index<size;index++){
					var setInfo = hisAtomServiceArray[index];
					var setInfoArray = setInfo.split("|");
					id1 = setInfoArray[1];
					id3 = setInfoArray[0];
					name2 = setInfoArray[2];
					name3 = setInfoArray[3];
					url = setInfoArray[4];
					if($("#div_"+menuDataTmp.substring(menuDataTmp.indexOf('P'),menuDataTmp.indexOf('n'))+"_right").find('a').length>=2){
				        		break;
				        }
			        if(menuDataTmp.indexOf(id1)>0&&menuDataTmp.indexOf(id3)>0&&!hrefData.contains(id3)){
			        	$("#div_"+id1).append("<div id='div_"+id1+"_right'>");
			        	$("#div_"+id1+"_right").append("<ul style='width: 10px; color: rgb(136, 136, 137); margin-right: 10px;float: left;'>|</ul>");
			        	$("#div_"+id1+"_right").append("<ul style='width: auto; color: rgb(136, 136, 137); margin-right: 10px;float: left;'>"+name2+"</ul>");
			        	$("#div_"+id1+"_right").append("<ul style='float: left;'>"+"-"+"&nbsp;&nbsp;&nbsp;"+"</ul>");
			        	if("PBL201635"==id3||"PBL20167001"==id3||"PBL20167002"==id3){
			        		$("#div_"+id1+"_right").append("<a id='atom"+id3+"'href='javascript:perbankAtomLocationTW(&#39;"+id3+"&#39;,&#39;"+url+"&#39;,dse_sessionId);historyViewSet(&#39;"+id3+"&#39;,&#39;"+id1+"&#39;,&#39;"+name2+"&#39;,&#39;"+name3+"&#39;,dse_sessionId,&#39;"+url+"&#39;);'style='color: #00A0E9;float: left;margin-right: 10px;' >"+name3+"</a>");
			        	}else{
			        		$("#div_"+id1+"_right").append("<a id='atom"+id3+"'href='javascript:perbankAtomLocationTW(&#39;"+id3+"&#39;,&#39;&#39;,dse_sessionId);historyViewSet(&#39;"+id3+"&#39;,&#39;"+id1+"&#39;,&#39;"+name2+"&#39;,&#39;"+name3+"&#39;,dse_sessionId,&#39;&#39;);'style='color: #00A0E9;float: left;margin-right: 10px;' >"+name3+"</a>");
			        	}
			        	$("#div_"+id1+"_right").append("</div>");
			        	hrefData[num++] = id3;
			        	if($("#div_"+id1+"_right").find('a').length<2){
			        		$("#atom"+id3).css("width","50%");
			        	}else{
			        		$("#div_"+id1+"_right").find('a').css("width","auto");
			        		var alist = new Array();
			        		alist = $("#div_"+id1+"_right a");
			        		$(alist[1]).css("width","25%");
			        		if(parseInt(Sys.ie)==9&&window.screen.width<=1440){
			        			$("#div_"+id1+"_right").css("width","800px"); 
			        		}else{
			        			$("#div_"+id1+"_right").css("width","1000px"); 
			        		}
			        	}
			        	count++;
			        }
			        $("#div_"+id1).next().children().css("margin-top","4px");//调整菜单区域间距，防止被挤出浮动展示框
			        $("#div_"+id1).show();
			        if(count >= 2){
			        	break;
			        }
	        	}
			}
		}
	}catch(e){
		//避免空catch
		}
	}	
	
}

