	var subWeb = top.document.frames ? top.document.frames["ebdp-floatdialog-iframe"].document : ifm.contentDocument; 
	jQuery(subWeb.body).css('overflow','auto');
	if(ifm != null && subWeb != null) { 
		var _height = subWeb.body.scrollHeight;
		ifm.height = _height;
		$middle.height(_height);
		/* 兼容窗口高度不够的情况，设置最大高度(窗口最大高度*(1-据上边框位置)-上边栏-下边栏) */
		var maxHeight = $(top.window).height();
		if(maxHeight!=null && maxHeight!=undefined){
			maxHeight = parseInt(maxHeight*0.9)-30-64;
			jQuery(ifm).css('max-height', maxHeight+'px'); 
			jQuery($middle).css('max-height', maxHeight+'px');
			jQuery(ifm).css('min-height','260px');
		}
	}
};
