/* 	
	遍历数组  拼接成字符串    
	 返回格式二级id~三级id| 二级id~三级id 
*/

function dealRecommentMenu(arr){
   var retStr = "";
	for(var i=0; i<arr.length; i++){
		var str = arr[i];
		retStr = retStr+"|"+getServicIdOfParent(str);
	}
	return retStr.substring(1,retStr.length);
}

/*
根据三级id(PBL0001) 获取 带有二级id 的字符串（PBL000~PBL0001） 
如果二级id未知，二级id默认采用tmp (tmp~PBLxxxx)
*/
function getServicIdOfParent(serviceId){
	for(var i=0; i<myquickAll_arr.length; i++){
		var strTmp = myquickAll_arr[i];
		if(strTmp.indexOf(serviceId)>-1){
			var tmpArr = strTmp.split("|");
			for(var j=0; j<tmpArr.length; j++){
				var seridStr = tmpArr[j];
				var arr = seridStr.split("~");
				if(arr[1] == serviceId){
					return seridStr;
				}
			}
		}
	}
	return "tmp~"+serviceId;
}

